{
  "contract_version": "1.1",
  "dependencies": [
    {
      "pkg": "stata",
      "purpose": "Cluster command",
      "source": "built-in"
    }
  ],
  "description": "Perform hierarchical clustering, generate dendrogram and cluster labels",
  "family": "multivariate",
