{
  "contract_version": "1.1",
  "dependencies": [
    {
      "pkg": "psmatch2",
      "purpose": "PSM matching",
      "source": "ssc"
    }
  ],
  "description": "Propensity score matching (1:1/1:N)",
  "family": "causal_inference",
  "id": "TG02",
  "inputs": [
    {
      "name": "data.csv",
      "required": true,
      "role": "main_dataset"
    }
  ],
  "level": "L1",
  "module": "G",
  "outputs": [
    {
      "description": "ATT results",
      "file": "table_TG02_att_result.csv",
      "type": "table"
    },
    {
      "description": "Balance after matching",
      "file": "table_TG02_balance_after.csv",
      "type": "table"
    },
    {
      "description": "Balance comparison",
      "file": "fig_TG02_balance_compare.png",
      "type": "figure"
    },
    {
      "description": "Matched data",
      "file": "data_TG02_matched.dta",
      "type": "data"
    },
    {
      "description": "Execution log",
      "file": "result.log",
      "type": "log"
    }
  ],
  "parameters": [
    {
      "description": "Treatment variable",
      "name": "__TREATMENT_VAR__",
      "required": true,
      "type": "string"
    },
    {
      "description": "Outcome variable",
      "name": "__OUTCOME_VAR__",
      "required": true,
      "type": "string"
    },
    {
      "description": "Covariates",
      "name": "__COVARIATES__",
      "required": true,
      "type": "list[string]"
    },
    {
      "description": "Number of neighbors",
      "name": "__N_NEIGHBORS__",
      "required": false,
      "type": "number"
    },
    {
      "description": "Caliper width",
      "name": "__CALIPER__",
      "required": false,
      "type": "number"
    },
    {
      "description": "With replacement",
      "name": "__WITH_REPLACE__",
      "required": false,
      "type": "string"
    },
    {
      "description": "Random seed (optional)",
      "name": "__SEED__",
      "required": false,
      "type": "integer"
    }
  ],
  "slug": "psm_match",
  "tags": [
    "psm",
    "matching",
    "att",
    "causal"
  ],
  "title": "PSM Match",
  "title_zh": "倾向得分匹配",
  "version": "2.0.1"
}
